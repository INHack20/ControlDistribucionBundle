<h1>Caso</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Procedencia</th>
            <td>{{ entity.procedencia }}</td>
        </tr>
        <tr>
            <th>Nrocausafiscal</th>
            <td>{{ entity.nroCausaFiscal }}</td>
        </tr>
        <tr>
            <th>Nrooficiofiscal</th>
            <td>{{ entity.nroOficioFiscal }}</td>
        </tr>
        <tr>
            <th>Nombreimputado</th>
            <td>{{ entity.nombreImputado }}</td>
        </tr>
        <tr>
            <th>Nombrevictima</th>
            <td>{{ entity.nombreVictima }}</td>
        </tr>
        <tr>
            <th>Pieza</th>
            <td>{{ entity.pieza }}</td>
        </tr>
        <tr>
            <th>Creado</th>
            <td>{{ entity.creado|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Actualizado</th>
            <td>{{ entity.actualizado|date('Y-m-d H:i:s') }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('caso') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('caso_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('caso_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
