<h1>Horario</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Dias</th>
            <td>{{ entity.dias }}</td>
        </tr>
        <tr>
            <th>Horainicio</th>
            <td>{{ entity.horaInicio|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Horafin</th>
            <td>{{ entity.horaFin|date('Y-m-d H:i:s') }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('horario') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('horario_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('horario_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
